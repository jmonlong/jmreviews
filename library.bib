  author = {Dixon, Anna L and Liang, Liming and Moffatt, Miriam F and Chen, Wei and Heath, Simon and Wong, Kenny C C and Taylor, Jenny and Burnett, Edward and Gut, Ivo and Farrall, Martin and Lathrop, G Mark and Abecasis, Goncalo R and Cookson, William O C},
