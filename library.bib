@article{Sedlazeck2017a,
  title = {Tools for Annotation and Comparison of Structural Variation.},
  author = {Sedlazeck, Fritz J. and Dhroso, Andi and Bodian, Dale L. and Paschall, Justin and Hermes, Farrah and Zook, Justin M.},
  year = {2017},
  journal = {F1000Research},
  volume = {6},
  number = {0},
  pages = {1795},
  issn = {2046-1402},
  doi = {10.12688/f1000research.12516.1},
  abstract = {The impact of structural variants (SVs) on a variety of organisms and diseases like cancer has become increasingly evident. Methods for SV detection when studying genomic differences across cells, individuals or populations are being actively developed. Currently, just a few methods are available to compare different SVs callsets, and no specialized methods are available to annotate SVs that account for the unique characteristics of these variant types. Here, we introduce SURVIVOR\textsubscript{a}nt, a tool that compares types and breakpoints for candidate SVs from different callsets and enables fast comparison of SVs to genomic features such as genes and repetitive regions, as well as to previously established SV datasets such as from the 1000 Genomes Project. As proof of concept we compared 16 SV callsets generated by different SV calling methods on a single genome, the Genome in a Bottle sample HG002 (Ashkenazi son), and annotated the SVs with gene annotations, 1000 Genomes Project SV calls, and four different types of repetitive regions. Computation time to annotate 134,528 SVs with 33,954 of annotations was 22 seconds on a laptop.},
  pmid = {29123647},
  keywords = {cnv,method,qc,sv,vcf},
  file = {/home/jmonlong/Documents/GoogleDriveC3G/zotero-library/Sedlazeck et al/sedlazeck_et_al_2017_tools_for_annotation_and_comparison_of_structural_variation.pdf}
}

