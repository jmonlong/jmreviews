@article{Vogt2014,
  title = {{{SVA}} Retrotransposon Insertion-Associated Deletion Represents a Novel Mutational Mechanism Underlying Large Genomic Copy Number Changes with Non-Recurrent Breakpoints},
  author = {Vogt, Julia and Bengesser, Kathrin and Claes, Kathleen B.M. and Wimmer, Katharina and Mautner, Victor Felix and {van Minkelen}, Rick and Legius, Eric and Brems, Hilde and Upadhyaya, Meena and H{\"o}gel, Josef and Lazaro, Conxi and Rosenbaum, Thorsten and Bammert, Simone and Messiaen, Ludwine and Cooper, David N. and {Kehrer-Sawatzki}, Hildegard},
  year = {2014},
  month = jun,
  journal = {Genome Biology},
  volume = {15},
  number = {6},
  pages = {R80},
  issn = {1474760X},
  doi = {10.1186/gb-2014-15-6-r80},
  abstract = {BACKGROUND: Genomic disorders are caused by copy number changes that may exhibit recurrent breakpoints processed by nonallelic homologous recombination. However, region-specific disease-associated copy number changes have also been observed which exhibit non-recurrent breakpoints. The mechanisms underlying these non-recurrent copy number changes have not yet been fully elucidated.\textbackslash n\textbackslash nRESULTS: We analyze large NF1 deletions with non-recurrent breakpoints as a model to investigate the full spectrum of causative mechanisms, and observe that they are mediated by various DNA double strand break repair mechanisms, as well as aberrant replication. Further, two of the 17 NF1 deletions with non-recurrent breakpoints, identified in unrelated patients, occur in association with the concomitant insertion of SINE/variable number of tandem repeats/Alu (SVA) retrotransposons at the deletion breakpoints. The respective breakpoints are refractory to analysis by standard breakpoint-spanning PCRs and are only identified by means of optimized PCR protocols designed to amplify across GC-rich sequences. The SVA elements are integrated within SUZ12P intron 8 in both patients, and were mediated by target-primed reverse transcription of SVA mRNA intermediates derived from retrotranspositionally active source elements. Both SVA insertions occurred during early postzygotic development and are uniquely associated with large deletions of 1 Mb and 867 kb, respectively, at the insertion sites.\textbackslash n\textbackslash nCONCLUSIONS: Since active SVA elements are abundant in the human genome and the retrotranspositional activity of many SVA source elements is high, SVA insertion-associated large genomic deletions encompassing many hundreds of kilobases could constitute a novel and as yet under-appreciated mechanism underlying large-scale copy number changes in the human genome.},
  isbn = {1465-6914 (Electronic)\textbackslash n1465-6906 (Linking)},
  pmid = {24958239},
  keywords = {cnv,mechanism,sv,sva,transposable-elements}
}

@article{Volik2003,
  type = {Journal Article},
  title = {End-Sequence Profiling: Sequence-Based Analysis of Aberrant Genomes.},
  author = {Volik, Stanislav and Zhao, Shaying and Chin, Koei and Brebner, John H and Herndon, David R and Tao, Quanzhou and Kowbel, David and Huang, Guiqing and Lapuk, Anna and Kuo, Wen-Lin L and Magrane, Gregg and De Jong, Pieter and Gray, Joe W and Collins, Colin},
  year = {2003},
  month = jun,
  journal = {Proceedings of the National Academy of Sciences of the United States of America},
  volume = {100},
  number = {13},
  pages = {7696--7701},
  issn = {0027-8424},
  doi = {10.1073/pnas.1232418100},
  abstract = {Genome rearrangements are important in evolution, cancer, and other diseases. Precise mapping of the rearrangements is essential for identification of the involved genes, and many techniques have been developed for this purpose. We show here that end-sequence profiling (ESP) is particularly well suited to this purpose. ESP is accomplished by constructing a bacterial artificial chromosome (BAC) library from a test genome, measuring BAC end sequences, and mapping end-sequence pairs onto the normal genome sequence. Plots of BAC end-sequences density identify copy number abnormalities at high resolution. BACs spanning structural aberrations have end pairs that map abnormally far apart on the normal genome sequence. These pairs can then be sequenced to determine the involved genes and breakpoint sequences. ESP analysis of the breast cancer cell line MCF-7 demonstrated its utility for analysis of complex genomes. End sequencing of approximately 8,000 clones (0.37-fold haploid genome clonal coverage) produced a comprehensive genome copy number map of the MCF-7 genome at better than 300-kb resolution and identified 381 genome breakpoints, a subset of which was verified by fluorescence in situ hybridization mapping and sequencing.},
  pmid = {12788976},
  keywords = {cancer,disease,esp,functional,method,sv},
  file = {/home/jmonlong/Documents/GoogleDriveC3G/zotero-library/Volik et al/volik_et_al_2003_end-sequence_profiling.pdf}
}

@article{Volinia2010,
  title = {Reprogramming of {{miRNA}} Networks in Cancer and Leukemia},
  author = {Volinia, Stefano and Galasso, Marco and Costinean, Stefan and Tagliavini, Luca and Gamberoni, Giacomo and Drusco, Alessandra and Marchesini, Jlenia and Mascellani, Nicoletta and Sana, Maria Elena and Jarour, Ramzey Abu and Desponts, Caroline and Teitell, Michael and Baffa, Raffaele and Aqeilan, Rami and Iorio, Marilena V. and Taccioli, Cristian and Garzon, Ramiro and Di Leva, Gianpiero and Fabbri, Muller and Catozzi, Marco and Previati, Maurizio and Ambs, Stefan and Palumbo, Tiziana and Garofalo, Michela and Veronese, Angelo and Bottoni, Arianna and Gasparini, Pierluigi and Harris, Curtis C. and Visone, Rosa and Pekarsky, Yuri and De La Chapelle, Albert and Bloomston, Mark and Dillhoff, Mary and Rassenti, Laura Z. and Kipps, Thomas J. and Huebner, Kay and Pichiorri, Flavia and Lenze, Dido and Cairo, Stefano and Buendia, Marie Annick and Pineau, Pascal and Dejean, Anne and Zanesi, Nicola and Rossi, Simona and Calin, George A. and Liu, Chang Gong and Palatini, Jeff and Negrini, Massimo and Vecchione, Andrea and Rosenberg, Anne and Croce, Carlo M.},
  year = {2010},
  journal = {Genome Research},
  volume = {20},
  number = {5},
  pages = {589--599},
  issn = {10889051},
  doi = {10.1101/gr.098046.109},
  abstract = {We studied miRNA profiles in 4419 human samples (3312 neoplastic, 1107 nonmalignant), corresponding to 50 normal tissues and 51 cancer types. The complexity of our database enabled us to perform a detailed analysis of microRNA (miRNA) activities. We inferred genetic networks from miRNA expression in normal tissues and cancer. We also built, for the first time, specialized miRNA networks for solid tumors and leukemias. Nonmalignant tissues and cancer networks displayed a change in hubs, the most connected miRNAs. hsa-miR-103/106 were downgraded in cancer, whereas hsa-miR-30 became most prominent. Cancer networks appeared as built from disjointed subnetworks, as opposed to normal tissues. A comparison of these nets allowed us to identify key miRNA cliques in cancer. We also investigated miRNA copy number alterations in 744 cancer samples, at a resolution of 150 kb. Members of miRNA families should be similarly deleted or amplified, since they repress the same cellular targets and are thus expected to have similar impacts on oncogenesis. We correctly identified hsa-miR-17/92 family as amplified and the hsa-miR-143/145 cluster as deleted. Other miRNAs, such as hsa-miR-30 and hsa-miR-204, were found to be physically altered at the DNA copy number level as well. By combining differential expression, genetic networks, and DNA copy number alterations, we confirmed, or discovered, miRNAs with comprehensive roles in cancer. Finally, we experimentally validated the miRNA network with acute lymphocytic leukemia originated in Mir155 transgenic mice. Most of miRNAs deregulated in these transgenic mice were located close to hsa-miR-155 in the cancer network.},
  isbn = {1135695191135},
  pmid = {20439436},
  keywords = {cancer,cnv,disease,microRNA,network,sequencing,sv},
  file = {/home/jmonlong/Documents/GoogleDriveC3G/zotero-library/Volinia et al/volinia_et_al_2010_reprogramming_of_mirna_networks_in_cancer_and_leukemia.pdf}
}

@techreport{Vollger2021,
  type = {Preprint},
  title = {Segmental Duplications and Their Variation in a Complete Human Genome},
  author = {Vollger, Mitchell R. and Guitart, Xavi and Dishuck, Philip C. and Mercuri, Ludovica and Harvey, William T. and Gershman, Ariel and Diekhans, Mark and Sulovari, Arvis and Munson, Katherine M. and Lewis, Alexandra M. and Hoekzema, Kendra and Porubsky, David and Li, Ruiyang and Nurk, Sergey and Koren, Sergey and Miga, Karen H. and Phillippy, Adam M. and Timp, Winston and Ventura, Mario and Eichler, Evan E.},
  year = {2021},
  month = may,
  institution = {{Genomics}},
  doi = {10.1101/2021.05.26.445678},
  abstract = {Despite their importance in disease and evolution, highly identical segmental duplications (SDs) have been among the last regions of the human reference genome (GRCh38) to be finished. Based on a complete telomere-to-telomere human genome (T2T-CHM13), we present the first comprehensive view of human SD organization. SDs account for nearly one-third of the additional sequence increasing the genome-wide estimate from 5.4\% to 7.0\% (218 Mbp). An analysis of 266 human genomes shows that 91\% of the new T2T-CHM13 SD sequence (68.3 Mbp) better represents human copy number. We find that SDs show increased singlenucleotide variation diversity when compared to unique regions; we characterize methylation signatures that correlate with duplicate gene transcription and predict 182 novel protein-coding gene candidates. We find that 63\% (35.11/55.7 Mbp) of acrocentric chromosomes consist of SDs distinct from rDNA and satellite sequences. Acrocentric SDs are 1.75-fold longer (p=0.00034) than other SDs, are frequently shared with autosomal pericentromeric regions, and are heteromorphic among human chromosomes. Comparing long-read assemblies from other human (n=12) and nonhuman primate (n=5) genomes, we use the T2T-CHM13 genome to systematically reconstruct the evolution and structural haplotype diversity of biomedically relevant (LPA, SMN) and duplicated genes (TBC1D3, SRGAP2C, ARHGAP11B) important in the expansion of the human frontal cortex. The analysis reveals unprecedented patterns of structural heterozygosity and massive evolutionary differences in SD organization between humans and their closest living relatives.},
  langid = {english},
  keywords = {chm13,human,repeat,segmental-duplication},
  file = {/home/jmonlong/Documents/GoogleDriveC3G/zotero-library/Vollger et al/vollger_et_al_2021_segmental_duplications_and_their_variation_in_a_complete_human_genome.pdf}
}

@article{Waaijenborg2009,
