  arxivid = {NIHMS150003},
  isbn = {1878-3686 (Electronic)\textbackslash n1535-6108 (Linking)},
  pmid = {20129251},
  keywords = {brain,cancer,cnv,gbm,human,sv,tcga}
}

@article{Verkerk1991,
