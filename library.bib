@article{Teng2016,
  title = {Accounting for {{GC-content}} Bias Reduces Systematic Errors and Batch Effects in {{ChIP-Seq}} Peak Callers},
  author = {Teng, Mingxiang and Irizarry, Rafael A.},
  year = {2016},
  journal = {bioRxiv},
  pages = {1--22},
  issn = {1549-5469},
  doi = {10.1101/090704},
  abstract = {ChIP-seq technology is widely used in biomedical and basic science research. The main application is the detection of genomic regions that bind to a protein of interest. ChIP-Seq studies rely on peak calling algorithms that attempt to infer protein-binding sites by detecting genomic regions associated with more mapped reads (coverage) than expected by chance as a result of the experimental protocol's lack of perfect specificity. We find that GC-content bias accounts for a substantial amount of variability in the observed coverage for ChIP-Seq experiments and that this variability leads to false positive peak calls. More concerning is that the GC-effect varies across experiments, with the effect strong enough to result in a substantial number of peaks called differently when different laboratories perform experiments on the same cell-line. Although solutions have been proposed for GC-bias corrections in other Next Generation Sequencing (NGS) applications, accounting for GC-content in ChIP-Seq data is challenging because the binding sites of interest tend to be more common in high GC-content regions, which confounds real biological signal with the unwanted variability we want to remove. To account for this challenge we introduce a statistical approach, based on a mixture model, that accounts for GC-content effects on both non-specific noise and signal induced by the binding site we seek to detect. The method can be used to account for this bias in binding quantification as well to improve existing peak calling algorithms. We use this approach to show improved consistency across laboratories.\textbackslash r\textbackslash n\textbackslash r\textbackslash n\textbackslash r\textbackslash n\textbackslash r\textbackslash n@mikelove 13h13 hours ago\textbackslash r\textbackslash nMike Love Retweeted bioRxiv\textbackslash r\textbackslash nFrom @mingxiangteng and @rafalab "accounting for GC in ChIP-Seq data is challenging... binding sites... more common in high GC regions"},
  pmid = {29025895},
  keywords = {bias,chip-seq,coverage,GC,sequencing},
