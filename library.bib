@article{Scoggins2006,
  title = {Gender-Related Differences in Outcome for Melanoma Patients.},
  author = {Scoggins, Charles R and Ross, Merrick I and Reintgen, Douglas S and Noyes, R Dirk and Goydos, James S and Beitsch, Peter D and Urist, Marshall M and Ariyan, Stephan and Sussman, Jeffrey J and Edwards, Michael J and Chagpar, Anees B and Martin, Robert C G and Stromberg, Arnold J and Hagendoorn, Lee and McMasters, Kelly M and {Sunbelt Melanoma Trial}},
  year = {2006},
  month = may,
  journal = {Annals of surgery},
  volume = {243},
  number = {5},
  pages = {693-8; discussion 698--700},
  issn = {0003-4932},
  doi = {10.1097/01.sla.0000216771.81362.6b},
  abstract = {OBJECTIVE To better understand the factors associated with the well-established gender difference in survival for patients with melanoma. SUMMARY BACKGROUND DATA Gender is an important factor in patients with cutaneous melanoma. Male patients have a worse outcome when compared with females. The reasons for this difference are poorly understood. METHODS This prospective multi-institutional study included patients aged 18 to 70 years with melanomas  or =1.0 mm Breslow thickness. Wide excision and sentinel lymph node (SLN) biopsy was performed in all patients. Clinicopathologic factors, including gender, were assessed and correlated with disease-free survival (DFS), distant disease-free survival (DDFS), and overall survival (OS). RESULTS A total of 3324 patients were included in the covariate analyses; 1829 patients had follow-up data available and were included in the survival analyses. Median follow-up was 30 months. On univariate analysis, men (n = 1906) were more likely than women to be older than 60 years (P  0.0001), have thicker melanomas (P  0.0001), have primary tumor regression (P = 0.0054), ulceration (P  0.0001), and axial primary tumor location (P  0.0001). On multivariate analysis, age (P = 0.0002), thickness (P  0.0001), ulceration (P = 0.015), and location (P  0.0001) remained significant in the model. There was no difference in the rate of SLN metastasis between men and women (P = 0.37) on multivariate analysis. When factors affecting survival were considered, the prognosis was worse for men as validated by lower DFS (P = 0.0005), DDFS (P  0.0001), and OS (P  0.0001). CONCLUSIONS Male gender is associated with a greater incidence of unfavorable primary tumor characteristics without an increased risk for nodal metastasis. Nonetheless, gender is an independent factor affecting survival.},
  pmid = {16633005},
  keywords = {bib-thesis,cancer,gender}
}

@article{Scott2016,
  title = {A Hot {{L1}} Retrotransposon Evades Somatic Repression and Initiates Human Colorectal Cancer},
  author = {Scott, Emma C. and Gardner, Eugene J. and Masood, Ashiq and Chuang, Nelson T. and Vertino, Paula M. and Devine, Scott E.},
  year = {2016},
  month = jun,
  journal = {Genome Research},
  volume = {26},
  number = {6},
  pages = {745--755},
  issn = {15495469},
  doi = {10.1101/gr.201814.115},
  abstract = {Although human LINE-1 (L1) elements are actively mobilized in many cancers, a role for somatic L1 retrotransposition in tumor initiation has not been conclusively demonstrated. Here, we identify a novel somatic L1 insertion in the APC tumor suppressor gene that provided us with a unique opportunity to determine whether such insertions can actually initiate colorectal cancer (CRC), and if so, how this might occur. Our data support a model whereby a hot L1 source element on Chromosome 17 of the patient's genome evaded somatic repression in normal colon tissues and thereby initiated CRC by mutating the APC gene. This insertion worked together with a point mutation in the second APC allele to initiate tumorigenesis through the classic two-hit CRC pathway. We also show that L1 source profiles vary considerably depending on the ancestry of an individual, and that population-specific hot L1 elements represent a novel form of cancer risk.},
  isbn = {1088-9051},
  pmid = {27197217},
  keywords = {cancer,disease,human,insertion,l1,repeat,sequencing,somatic,sv,transposable-elements},
  file = {/home/jmonlong/Documents/GoogleDriveC3G/zotero-library/Scott et al/scott_et_al_2016_a_hot_l1_retrotransposon_evades_somatic_repression_and_initiates_human.pdf}
}

