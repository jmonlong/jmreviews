@article{Geoffroy2018,
  title = {{{AnnotSV}}: An Integrated Tool for Structural Variations Annotation},
  shorttitle = {{{AnnotSV}}},
  author = {Geoffroy, V{\'e}ronique and Herenger, Yvan and Kress, Arnaud and Stoetzel, Corinne and Piton, Am{\'e}lie and Dollfus, H{\'e}l{\`e}ne and Muller, Jean},
