  year = {2015},
  month = jan,
  journal = {Nucleic Acids Research},
  volume = {43},
  number = {D1},
  pages = {D893-D899},
  issn = {13624962},
  doi = {10.1093/nar/gku943},
  abstract = {Epilepsy is one of the most prevalent chronic neurological disorders, afflicting about 3.5-6.5 per 1000 children and 10.8 per 1000 elderly people. With intensive effort made during the last two decades, numerous genes and mutations have been published to be associated with the disease. An organized resource integrating and annotating the ever-increasing genetic data will be imperative to acquire a global view of the cutting-edge in epilepsy research. Herein, we developed EpilepsyGene (http://61.152.91.49/EpilepsyGene). It contains cumulative to date 499 genes and 3931 variants associated with 331 clinical phenotypes collected from 818 publications. Furthermore, in-depth data mining was performed to gain insights into the understanding of the data, including functional annotation, gene prioritization, functional analysis of prioritized genes and overlap analysis focusing on the comorbidity. An intuitive web interface to search and browse the diversified genetic data was also developed to facilitate access to the data of interest. In general, EpilepsyGene is designed to be a central genetic database to provide the research community substantial convenience to uncover the genetic basis of epilepsy.},
  isbn = {1362-4962 (Electronic)\textbackslash r0305-1048 (Linking)},
  langid = {english},
  pmid = {25324312},
  keywords = {bib-epipopsv,bib-reppopsv,bib-thesis,disease,epilepsy,functional},
  file = {/home/jmonlong/Documents/GoogleDriveC3G/zotero-library/Ran et al/ran_et_al_2015_epilepsygene.pdf;/home/jmonlong/Documents/GoogleDriveC3G/zotero-library/Ran et al/ran_et_al_2015_epilepsygene2.pdf}
}

