@article{Rand2017,
  title = {Coordinates and Intervals in Graph-Based Reference Genomes.},
  author = {Rand, Knut Dagestad and Grytten, Ivar and Nederbragt, Alexander J and Storvik, Geir Olve and Glad, Ingrid Kristine and Sandve, Geir Kjetil},
