@article{Robinson2010a,
  title = {{{edgeR}}: A {{Bioconductor}} Package for Differential Expression Analysis of Digital Gene Expression Data.},
  author = {Robinson, Mark D and McCarthy, Davis J and Smyth, Gordon K},
  year = {2010},
