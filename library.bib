@article{Mostovoy2020,
  title = {Genome Mapping Resolves Structural Variation within Segmental Duplications Associated with Microdeletion/Microduplication Syndromes},
  author = {Mostovoy, Yulia and Yilmaz, Feyza and Chow, Stephen K and Chu, Catherine and Lin, Chin and Geiger, Elizabeth A and Meeks, Naomi J L and Chatfield, Kathryn C and Coughlin, Curtis R and Kwok, Pui-Yan and Shaikh, Tamim H},
  year = {2020},
  month = may,
  journal = {bioRxiv},
  doi = {10.1101/2020.04.30.071449},
  abstract = {Segmental duplications (SDs) are a class of long, repetitive DNA elements whose paralogs share a high level of sequence similarity with each other. SDs mediate chromosomal rearrangements that lead to structural variation in the general population as well as genomic disorders associated with multiple congenital anomalies, including the 7q11.23 (Williams-Beuren Syndrome, WBS), 15q13.3, and 16p12.2 microdeletion syndromes. These three genomic regions, and the SDs within them, have been previously analyzed in a small number of individuals. However, population-level studies have been lacking because most techniques used for analyzing these complex regions are both labor- and cost-intensive. In this study, we present a high-throughput technique to genotype complex structural variation using a single molecule, long-range optical mapping approach. We identified novel structural variants (SVs) at 7q11.23, 15q13.3 and 16p12.2 using optical mapping data from 154 phenotypically normal individuals from 26 populations comprising 5 super-populations. We detected several novel SVs for each locus, some of which had significantly different prevalence between populations. Additionally, we refined the microdeletion breakpoints located within complex SDs in two patients with WBS, one patient with 15q13.3, and one patient with 16p12.2 microdeletion syndromes. The population-level data presented here highlights the extreme diversity of large and complex SVs within SD-containing regions. The approach we outline will greatly facilitate the investigation of the role of inter-SD structural variation as a driver of chromosomal rearrangements and genomic disorders.},
  langid = {english},
  file = {/home/jmonlong/Documents/GoogleDriveC3G/zotero-library/Mostovoy et al/mostovoy_et_al_2020_genome_mapping_resolves_structural_variation_within_segmental_duplications.pdf}
}

@article{Mu2015,
  title = {{{VarSim}}: {{A}} High-Fidelity Simulation and Validation Framework for High-Throughput Genome Sequencing with Cancer Applications},
  author = {Mu, John C. and Mohiyuddin, Marghoob and Li, Jian and Bani Asadi, Narges and Gerstein, Mark B. and Abyzov, Alexej and Wong, Wing H. and Lam, Hugo Y.K.},
