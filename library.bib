@article{Iqbal2012,
  title = {De Novo Assembly and Genotyping of Variants Using Colored de {{Bruijn}} Graphs.},
  author = {Iqbal, Zamin and Caccamo, Mario and Turner, Isaac and Flicek, Paul and McVean, Gil},
