}

@article{Hodges2011,
  title = {Directional {{DNA}} Methylation Changes and Complex Intermediate States Accompany Lineage Specificity in the Adult Hematopoietic Compartment},
  author = {Hodges, Emily and Molaro, Antoine and Dos Santos, Camila O. and Thekkat, Pramod and Song, Qiang and Uren, Philip J. and Park, Jin and Butler, Jason and Rafii, Shahin and McCombie, W. Richard and Smith, Andrew D. and Hannon, Gregory J.},
  year = {2011},
  journal = {Molecular Cell},
  volume = {44},
  number = {1},
  pages = {17--28},
  publisher = {{Elsevier Inc.}},
  issn = {10972765},
  doi = {10.1016/j.molcel.2011.08.026},
  abstract = {DNA methylation has been implicated as an epigenetic component of mechanisms that stabilize cell-fate decisions. Here, we have characterized the methylomes of human female hematopoietic stem/progenitor cells (HSPCs) and mature cells from the myeloid and lymphoid lineages. Hypomethylated regions (HMRs) associated with lineage-specific genes were often methylated in the opposing lineage. In HSPCs, these sites tended to show intermediate, complex patterns that resolve to uniformity upon differentiation, by increased or decreased methylation. Promoter HMRs shared across diverse cell types typically display a constitutive core that expands and contracts in a lineage-specific manner to fine-tune the expression of associated genes. Many newly identified intergenic HMRs, both constitutive and lineage specific, were enriched for factor binding sites with an implied role in genome organization and regulation of gene expression, respectively. Overall, our studies represent an important reference data set and provide insights into directional changes in DNA methylation as cells adopt terminal fates. \textcopyright{} 2011 Elsevier Inc.},
  isbn = {1097-2765},
  pmid = {21924933}
}

@article{Hodgkinson2011,
  title = {Variation in the Mutation Rate across Mammalian Genomes.},
  author = {Hodgkinson, Alan and {Eyre-Walker}, Adam},
  year = {2011},
  month = nov,
  journal = {Nature reviews. Genetics},
  volume = {12},
  number = {11},
  pages = {756--66},
  publisher = {{Nature Publishing Group}},
  issn = {1471-0064},
  doi = {10.1038/nrg3098},
  abstract = {It has been known for many years that the mutation rate varies across the genome. However, only with the advent of large genomic data sets is the full extent of this variation becoming apparent. The mutation rate varies over many different scales, from adjacent sites to whole chromosomes, with the strongest variation seen at the smallest scales. Some of these patterns have clear mechanistic bases, but much of the rate variation remains unexplained, and some of it is deeply perplexing. Variation in the mutation rate has important implications in evolutionary biology and underexplored implications for our understanding of hereditary disease and cancer.},
  pmid = {21969038}
}

@article{Hollox2008,
