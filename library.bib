@article{Nagasaki2015,
  title = {Rare Variant Discovery by Deep Whole-Genome Sequencing of 1,070 {{Japanese}} Individuals},
  author = {Nagasaki, Masao and Yasuda, Jun and Katsuoka, Fumiki and Nariai, Naoki and Kojima, Kaname and Kawai, Yosuke and {Yamaguchi-Kabata}, Yumi and Yokozawa, Junji and Danjoh, Inaho and Saito, Sakae and Sato, Yukuto and Mimori, Takahiro and Tsuda, Kaoru and Saito, Rumiko and Pan, Xiaoqing and Nishikawa, Satoshi and Ito, Shin and Kuroki, Yoko and Tanabe, Osamu and Fuse, Nobuo and Kuriyama, Shinichi and Kiyomoto, Hideyasu and Hozawa, Atsushi and Minegishi, Naoko and Engel, James Douglas and Kinoshita, Kengo and Kure, Shigeo and Yaegashi, Nobuo and Yamamoto, Masayuki and Tsuboi, Akito and Nagami, Fuji and Kawame, Hiroshi and Tomita, Hiroaki and Tsuji, Ichiro and Nakaya, Jun and Sugawara, Junichi and Suzuki, Kichiya and Kikuya, Masahiro and Abe, Michiaki and Nakaya, Naoki and Osumi, Noriko and Yamashita, Riu and Ogishima, Soichi and Takai, Takako and Tominaga, Teiji and Taki, Yasuyuki and Suzuki, Yoichi},
