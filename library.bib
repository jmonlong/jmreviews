@article{Torkamani2018,
  title = {The Personal and Clinical Utility of Polygenic Risk Scores},
  author = {Torkamani, Ali},
  year = {2018},
  pages = {10},
  doi = {10.1038/s41576-018-0018-x},
  abstract = {Initial expectations for genome-wide association studies were high, as such studies promised to rapidly transform personalized medicine with individualized disease risk predictions, prevention strategies and treatments. Early findings, however, revealed a more complex genetic architecture than was anticipated for most common diseases \textemdash{} complexity that seemed to limit the immediate utility of these findings. As a result, the practice of utilizing the DNA of an individual to predict disease has been judged to provide little to no useful information. Nevertheless, recent efforts have begun to demonstrate the utility of polygenic risk profiling to identify groups of individuals who could benefit from the knowledge of their probabilistic susceptibility to disease. In this context, we review the evidence supporting the personal and clinical utility of polygenic risk profiling.},
  langid = {english},
  keywords = {polygenic-risk-score,review},
  file = {/home/jmonlong/Documents/GoogleDriveC3G/zotero-library/Torkamani/torkamani_2018_the_personal_and_clinical_utility_of_polygenic_risk_scores.pdf}
