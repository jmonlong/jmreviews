}

@article{Galizia2015,
  ids = {galizia2015bjn},
  title = {{{CHD2}} Variants Are a Risk Factor for Photosensitivity in Epilepsy},
  author = {Galizia, Elizabeth C. and Myers, Candace T. and Leu, Costin and {de Kovel}, Carolien G. F. and Afrikanova, Tatiana and {Cordero-Maldonado}, Maria L.orena and Martins, Teresa G. and Jacmin, Maxime and Drury, Suzanne and Krishna Chinthapalli, V. and Muhle, Hiltrud and Pendziwiat, Manuela and Sander, Thomas and Ruppert, Ann-Kathrin and M{\o}ller, Rikke S. and Thiele, Holger and Krause, Roland and Schubert, Julian and Lehesjoki, Anna-Elina and N{\"u}rnberg, Peter and Lerche, Holger and Palotie, Aarno and Coppola, Antonietta and Striano, Salvatore and Gaudio, Luigi D.el and Boustred, Christopher and Schneider, Amy L. and Lench, Nicholas and {Jocic-Jakubi}, Bosanka and Covanis, Athanasios and Capovilla, Giuseppe and Veggiotti, Pierangelo and Piccioli, Marta and Parisi, Pasquale and Cantonetti, Laura and Sadleir, Lynette G. and Mullen, Saul A. and Berkovic, Samuel F. and Stephani, Ulrich and Helbig, Ingo and Crawford, Alexander D. and Esguerra, Camila V. and {Kasteleijn-Nolst Trenit{\'e}}, Dorothee G. A. and Koeleman, Bobby P. C. and Mefford, Heather C. and Scheffer, Ingrid E. and Sisodiya, Sanjay M.},
  year = {2015},
  month = may,
  journal = {Brain},
  volume = {138},
  number = {5},
  pages = {1198--1208},
  issn = {0006-8950},
  doi = {10.1093/brain/awv052},
  abstract = {Photosensitivity is a heritable abnormal cortical response to flickering light, manifesting as particular electroencephalographic changes, with or without seizures. Photosensitivity is prominent in a very rare epileptic encephalopathy due to de novo CHD2 mutations, but is also seen in epileptic encephalopathies due to other gene mutations. We determined whether CHD2 variation underlies photosensitivity in common epilepsies, specific photosensitive epilepsies and individuals with photosensitivity without seizures. We studied 580 individuals with epilepsy and either photosensitive seizures or abnormal photoparoxysmal response on electroencephalography, or both, and 55 individuals with photoparoxysmal response but no seizures. We compared CHD2 sequence data to publicly available data from 34 427 individuals, not enriched for epilepsy. We investigated the role of unique variants seen only once in the entire data set. We sought CHD2 variants in 238 exomes from familial genetic generalized epilepsies, and in other public exome data sets. We identified 11 unique variants in the 580 individuals with photosensitive epilepsies and 128 unique variants in the 34 427 controls: unique CHD2 variation is over-represented in cases overall (P = 2\textperiodcentered 17 \texttimes{} 10(-5)). Among epilepsy syndromes, there was over-representation of unique CHD2 variants (3/36 cases) in the archetypal photosensitive epilepsy syndrome, eyelid myoclonia with absences (P = 3\textperiodcentered 50 \texttimes{} 10(-4)). CHD2 variation was not over-represented in photoparoxysmal response without seizures. Zebrafish larvae with chd2 knockdown were tested for photosensitivity. Chd2 knockdown markedly enhanced mild innate zebrafish larval photosensitivity. CHD2 mutation is the first identified cause of the archetypal generalized photosensitive epilepsy syndrome, eyelid myoclonia with absences. Unique CHD2 variants are also associated with photosensitivity in common epilepsies. CHD2 does not encode an ion channel, opening new avenues for research into human cortical excitability.},
  isbn = {00068950 (ISSN)},
  pmid = {25783594},
  keywords = {bib-epipopsv,bib-reppopsv,bib-thesis}
}

@article{Gambin2017,
  title = {Homozygous and Hemizygous {{CNV}} Detection from Exome Sequencing Data in a {{Mendelian}} Disease Cohort},
  author = {Gambin, Tomasz and Akdemir, Zeynep C. and Yuan, Bo and Gu, Shen and Chiang, Theodore and Carvalho, Claudia M.B. and Shaw, Chad and Jhangiani, Shalini and Boone, Philip M. and Eldomery, Mohammad K. and Karaca, Ender and Bayram, Yavuz and {Stray-Pedersen}, Asbj{\o}rg and Muzny, Donna and Charng, Wu Lin and Bahrambeigi, Vahid and Belmont, John W. and Boerwinkle, Eric and Beaudet, Arthur L. and Gibbs, Richard A. and Lupski, James R.},
  year = {2017},
  journal = {Nucleic acids research},
  volume = {45},
  number = {4},
  pages = {1633--1648},
  issn = {13624962},
  doi = {10.1093/nar/gkw1237},
  abstract = {We developed an algorithm, HMZDelFinder, that uses whole exome sequencing (WES) data to identify rare and intragenic homozygous and hemizygous (HMZ) deletions that may represent complete lossof- function of the indicated gene. HMZDelFinder was applied to 4866 samples in the Baylor\textendash Hopkins Center for Mendelian Genomics (BHCMG) cohort and detected 773 HMZ deletion calls (567 homozygous or 206 hemizygous) with an estimated sensitivity of 86.5\% (82\% for single-exonic and 88\% for multiexonic calls) and precision of 78\% (53\% singleexonic and 96\% for multi-exonic calls). Out of 773 HMZDelFinder-detected deletion calls, 82 were subjected to array comparative genomic hybridization (aCGH) and/or breakpoint PCR and 64 were confirmed. These include 18 single-exon deletions out of which 8 were exclusively detected by HMZDelFinder and not by any of seven other CNV detection tools examined. Further investigation of the 64 validated deletion calls revealed at least 15 pathogenic HMZ deletions. Of those, 7 accounted for 17\textendash 50\% of pathogenic CNVs in different disease cohorts where 7.1\textendash 11\% of the molecular diagnosis solved rate was attributed to CNVs. In summary, we present an algorithm to detect rare, intragenic, single-exon deletion CNVs using WES data; this tool can be useful for disease gene discovery efforts and clinical WES analyses.},
  pmid = {27980096},
  keywords = {cnv,disease,exome,human,rare-disease,sv}
}

