@article{Goodson2011,
  title = {Alternative {{mRNA}} Splicing of Corepressors Generates Variants That Play Opposing Roles in Adipocyte Differentiation.},
  author = {Goodson, Michael L and Mengeling, Brenda J and a Jonas, Brian and Privalsky, Martin L},
  year = {2011},
  month = dec,
  journal = {The Journal of biological chemistry},
  volume = {286},
  number = {52},
  pages = {44988--99},
  issn = {1083-351X},
  doi = {10.1074/jbc.M111.291625},
  abstract = {The SMRT and NCoR corepressors partner with, and help mediate repression by, a wide variety of nuclear receptors and non-receptor transcription factors. Both SMRT and NCoR are expressed by alternative mRNA splicing, resulting in the production of a series of interrelated corepressor variants that differ in their tissue distribution and in their biochemical properties. We report here that different corepressor splice variants can exert opposing transcriptional and biological effects during adipocyte differentiation. Most notably, the NCoR{$\omega$} splice variant inhibits, whereas the NCoR{$\delta$} splice variant promotes, adipogenesis. Furthermore, the ratio of NCoR{$\omega$} to NCoR{$\delta$} decreases during adipogenic differentiation. We propose that this alteration in corepressor splicing helps convert the cellular transcriptional program from one that maintains the pre-adipocyte in an undifferentiated state to a new transcriptional context that promotes differentiation and helps establish the proper physiology of the mature adipocyte.},
  pmid = {22065574}
}

