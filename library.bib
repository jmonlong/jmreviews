@article{Walport2014,
  title = {Human {{UTY}}({{KDM6C}}) {{Is}} a {{Male-specific N}} {$\epsilon$} -{{Methyl Lysyl Demethylase}}},
  author = {Walport, Louise J. and Hopkinson, Richard J. and Vollmar, Melanie and Madden, Sarah K. and Gileadi, Carina and Oppermann, Udo and Schofield, Christopher J. and Johansson, Catrine},
  year = {2014},
  month = jun,
  journal = {Journal of Biological Chemistry},
  volume = {289},
  number = {26},
  pages = {18302--18313},
  issn = {0021-9258},
  doi = {10.1074/jbc.M114.555052},
  abstract = {The Jumonji C lysine demethylases (KDMs) are 2-oxoglutarate- and Fe(II)-dependent oxygenases. KDM6A (UTX) and KDM6B (JMJD3) are KDM6 subfamily members that catalyze demethylation of N({$\epsilon$})-methylated histone 3 lysine 27 (H3K27), a mark important for transcriptional repression. Despite reports stating that UTY(KDM6C) is inactive as a KDM, we demonstrate by biochemical studies, employing MS and NMR, that UTY(KDM6C) is an active KDM. Crystallographic analyses reveal that the UTY(KDM6C) active site is highly conserved with those of KDM6B and KDM6A. UTY(KDM6C) catalyzes demethylation of H3K27 peptides in vitro, analogously to KDM6B and KDM6A, but with reduced activity, due to point substitutions involved in substrate binding. The results expand the set of human KDMs and will be of use in developing selective KDM inhibitors.},
  isbn = {1083-351X (Electronic)\textbackslash r0021-9258 (Linking)},
  langid = {english},
  pmid = {24798337},
  keywords = {bib-loy,bib-thesis}
}

