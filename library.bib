  keywords = {mechanism,review,sv},
  file = {/home/jmonlong/Documents/GoogleDriveC3G/zotero-library/Sasaki et al/sasaki_et_al_2010_genome_destabilization_by_homologous_recombination_in_the_germ_line.pdf}
}

@article{Sathirapongsasuti2011,
  title = {Exome Sequencing-Based Copy-Number Variation and Loss of Heterozygosity Detection: {{ExomeCNV}}.},
  author = {Sathirapongsasuti, Jarupon Fah and Lee, Hane and Horst, Basil a J and Brunner, Georg and Cochran, Alistair J and Binder, Scott and Quackenbush, John and Nelson, Stanley F},
  year = {2011},
