}

@article{Sharp2008,
  title = {A Recurrent 15q13.3 Microdeletion Syndrome Associated with Mental Retardation and Seizures.},
  author = {Sharp, Andrew J and Mefford, Heather C and Li, Kelly and Baker, Carl and Skinner, Cindy and Stevenson, Roger E and Schroer, Richard J and Novara, Francesca and De Gregori, Manuela and Ciccone, Roberto and Broomer, Adam and Casuga, Iris and Wang, Yu and Xiao, Chunlin and Barbacioru, Catalin and Gimelli, Giorgio and Bernardina, Bernardo Dalla and Torniero, Claudia and Giorda, Roberto and Regan, Regina and Murday, Victoria and Mansour, Sahar and Fichera, Marco and Castiglia, Lucia and Failla, Pinella and Ventura, Mario and Jiang, Zhaoshi and Cooper, Gregory M and Knight, Samantha J L and Romano, Corrado and Zuffardi, Orsetta and Chen, Caifu and Schwartz, Charles E and Eichler, Evan E},
  year = {2008},
  journal = {Nature genetics},
  volume = {40},
  number = {3},
  pages = {322--328},
  issn = {1061-4036},
  doi = {10.1038/ng.93},
  abstract = {We report a recurrent microdeletion syndrome causing mental retardation, epilepsy and variable facial and digital dysmorphisms. We describe nine affected individuals, including six probands: two with de novo deletions, two who inherited the deletion from an affected parent and two with unknown inheritance. The proximal breakpoint of the largest deletion is contiguous with breakpoint 3 (BP3) of the Prader-Willi and Angelman syndrome region, extending 3.95 Mb distally to BP5. A smaller 1.5-Mb deletion has a proximal breakpoint within the larger deletion (BP4) and shares the same distal BP5. This recurrent 1.5-Mb deletion contains six genes, including a candidate gene for epilepsy (CHRNA7) that is probably responsible for the observed seizure phenotype. The BP4-BP5 region undergoes frequent inversion, suggesting a possible link between this inversion polymorphism and recurrent deletion. The frequency of these microdeletions in mental retardation cases is approximately 0.3\% (6/2,082 tested), a prevalence comparable to that of Williams, Angelman and Prader-Willi syndromes.},
  isbn = {1546-1718 (Electronic)\textbackslash n1061-4036 (Linking)},
  pmid = {18278044},
  keywords = {ACGH,cnv,disease,epilepsy,functional,human,sv},
  file = {/home/jmonlong/Documents/GoogleDriveC3G/zotero-library/Sharp et al/sharp_et_al_2008_a_recurrent_15q13.pdf}
}

@article{She2006,
  title = {A Preliminary Comparative Analysis of Primate Segmental Duplications Shows Elevated Substitution Rates and a Great-Ape Expansion of Intrachromosomal Duplications.},
  author = {She, Xinwei and Liu, Ge and Ventura, Mario and Zhao, Shaying and Misceo, Doriana and Roberto, Roberta and Cardone, Maria Francesca and Rocchi, Mariano and {NISC Comparative Sequencing Program} and Green, Eric D and Archidiacano, Nicoletta and Eichler, Evan E},
  year = {2006},
  month = may,
  journal = {Genome research},
  volume = {16},
  number = {5},
  pages = {576--83},
  issn = {1088-9051},
  doi = {10.1101/gr.4949406},
  abstract = {Compared with other sequenced animal genomes, human segmental duplications appear larger, more interspersed, and disproportionately represented as high-sequence identity alignments. Global sequence divergence estimates of human duplications have suggested an expansion relatively recently during hominoid evolution. Based on primate comparative sequence analysis of 37 unique duplication-transition regions, we establish a molecular clock for their divergence that shows a significant increase in their effective substitution rate when compared with unique genomic sequence. Fluorescent in situ hybridization (FISH) analyses from 1053 random nonhuman primate BACs indicate that great-ape species have been enriched for interspersed segmental duplications compared with representative Old World and New World monkeys. These findings support computational analyses that show a 12-fold excess of recent (98\%) intrachromosomal duplications when compared with duplications between nonhomologous chromosomes. These architectural shifts in genomic structure and elevated substitution rates have important implications for the emergence of new genes, gene-expression differences, and structural variation among humans and great apes.},
  pmid = {16606706},
  keywords = {bib-thesis,cnv,human,segmental-duplication,sv}
}

