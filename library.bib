@article{Bruder2008,
  title = {Phenotypically Concordant and Discordant Monozygotic Twins Display Different {{DNA}} Copy-Number-Variation Profiles.},
  author = {Bruder, Carl E G and Piotrowski, Arkadiusz and Gijsbers, Antoinet a C J and Andersson, Robin and Erickson, Stephen and {Diaz de St{\aa}hl}, Teresita and Menzel, Uwe and Sandgren, Johanna and {von Tell}, Desiree and Poplawski, Andrzej and Crowley, Michael and Crasto, Chiquito and Partridge, E Christopher and Tiwari, Hemant and Allison, David B and Komorowski, Jan and {van Ommen}, Gert-Jan B and Boomsma, Dorret I and Pedersen, Nancy L and {den Dunnen}, Johan T and Wirdefeldt, Karin and Dumanski, Jan P},
