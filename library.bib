@article{Armstrong2019,
  title = {Progressive Alignment with {{Cactus}}: A Multiple-Genome Aligner for the Thousand-Genome Era},
  shorttitle = {Progressive Alignment with {{Cactus}}},
  author = {Armstrong, Joel and Hickey, Glenn and Diekhans, Mark and Deran, Alden and Fang, Qi and Xie, Duo and Feng, Shaohong and Stiller, Josefin and Genereux, Diane and Johnson, Jeremy and Marinescu, Voichita Dana and Haussler, David and Alf{\"o}ldi, Jessica and {Lindblad-Toh}, Kerstin and Karlsson, Elinor and Jarvis, Erich D and Zhang, Guojie and Paten, Benedict},
  year = {2019},
  month = oct,
  journal = {bioRxiv},
  doi = {10.1101/730531},
  abstract = {Cactus, a reference-free multiple genome alignment program, has been shown to be highly accurate, but the existing implementation scales poorly with increasing numbers of genomes, and struggles in regions of highly duplicated sequence. We describe progressive extensions to Cactus that enable reference-free alignment of tens to thousands of large vertebrate genomes while maintaining high alignment quality. We show that Cactus is capable of scaling to hundreds of genomes and beyond by describing results from an alignment of over 600 amniote genomes, which is to our knowledge the largest multiple vertebrate genome alignment yet created. Further, we show improvements in orthology resolution leading to downstream improvements in annotation.},
  langid = {english},
  keywords = {alignment,cactus,comparative-genomics,whole-genome},
  file = {/home/jmonlong/Documents/GoogleDriveC3G/zotero-library/Armstrong et al/armstrong_et_al_2019_progressive_alignment_with_cactus.pdf}
}

@article{Arseneault2017,
  title = {Loss of Chromosome {{Y}} Leads to down Regulation of {{KDM5D}} and {{KDM6C}} Epigenetic Modifiers in Clear Cell Renal Cell Carcinoma},
  author = {Arseneault, Madeleine and Monlong, Jean and Vasudev, Naveen S and Laskar, Ruhina S and Safisamghabadi, Maryam and Harnden, Patricia and Egevad, Lars and Nourbehesht, Nazanin and Panichnantakul, Pudchalaluck and Holcatova, Ivana and Brisuda, Antonin and Janout, Vladimir and Kollarova, Helena and Foretova, Lenka and Navratilova, Marie and Mates, Dana and Jinga, Viorel and Zaridze, David and Mukeria, Anush and Jandaghi, Pouria and Brennan, Paul and Brazma, Alvis and Tost, Jorg and Scelo, Ghislaine and Banks, Rosamonde E and Lathrop, Mark and Bourque, Guillaume and Riazalhosseini, Yasser},
  year = {2017},
  month = mar,
  journal = {Scientific Reports},
  volume = {7},
  pages = {44876},
  issn = {2045-2322},
  doi = {10.1038/srep44876},
  abstract = {Recent genomic studies of sporadic clear cell renal cell carcinoma (ccRCC) have uncovered novel driver genes and pathways. Given the unequal incidence rates among men and women (male:female incidence ratio approaches 2:1), we compared the genome-wide distribution of the chromosomal abnormalities in both sexes. We observed a higher frequency for the somatic recurrent chromosomal copy number variations (CNVs) of autosomes in male subjects, whereas somatic loss of chromosome X was detected exclusively in female patients (17.1\%). Furthermore, somatic loss of chromosome Y (LOY) was detected in about 40\% of male subjects, while mosaic LOY was detected in DNA isolated from peripheral blood in 9.6\% of them, and was the only recurrent CNV in constitutional DNA samples. LOY in constitutional DNA, but not in tumor DNA was associated with older age. Amongst Y-linked genes that were downregulated due to LOY, KDM5D and KDM6C epigenetic modifiers have functionally-similar X-linked homologs whose deficiency is involved in ccRCC progression. Our findings establish somatic LOY as a highly recurrent genetic defect in ccRCC that leads to downregulation of hitherto unsuspected epigenetic factors, and suggest that different mechanisms may underlie the somatic and mosaic LOY observed in tumors and peripheral blood, respectively.},
  pmid = {28332632},
  keywords = {bib-thesis,mypublications},
  file = {/home/jmonlong/Documents/GoogleDriveC3G/zotero-library/Arseneault et al/arseneault_et_al_2017_loss_of_chromosome_y_leads_to_down_regulation_of_kdm5d_and_kdm6c_epigenetic.pdf}
}

