  pmid = {23260136},
  keywords = {cnv,disease,hotspot,neuro,sequencing,sv,whole-genome},
  file = {/home/jmonlong/Documents/GoogleDriveC3G/zotero-library/Michaelson et al/michaelson_et_al_2012_whole-genome_sequencing_in_autism_identifies_hot_spots_for_de_novo_germline.pdf}
}

